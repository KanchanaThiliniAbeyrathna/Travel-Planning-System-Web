<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App1\ExampleBundle\Entity\Trip" table="trip">
    <indexes>
      <index name="fk_trip_user1_idx" columns="user_id"/>
      <index name="fk_trip_city1_idx" columns="city_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="city" target-entity="City" fetch="LAZY">
      <join-columns>
        <join-column name="city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="visitingplace" target-entity="Visitingplace" mapped-by="trip" fetch="LAZY"/>
    <many-to-many field="hotel" target-entity="Hotel" mapped-by="trip" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
